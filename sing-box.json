{
  "version": "1.14.0",
  "log": {
    "level": "info",
    "timestamp": true
  },
  "dns": {
    "servers": [
      {
        "tag": "dns-direct",
        "address": "tls://8.8.8.8:853",
        "detour": "direct"
      },
      {
        "tag": "dns-proxy",
        "address": "tls://1.1.1.1:853",
        "detour": "select"
      }
    ],
    "rules": [
      {
        "outbound": "any",
        "server": "dns-direct"
      },
      {
        "geosite": "cn",
        "server": "dns-direct"
      },
      {
        "clash_mode": "direct",
        "server": "dns-direct"
      },
      {
        "clash_mode": "global",
        "server": "dns-proxy"
      }
    ],
    "final": "dns-direct",
    "strategy": "prefer_ipv4"
  },
  "inbounds": [
    {
      "type": "tun",
      "tag": "tun-in",
      "interface_name": "tun0",
      "mtu": 9000,
      "stack": "mixed",
      "endpoint_independent_nat": true,
      "strict_route": true,
      "inet4_address": "172.19.0.1/30",
      "inet6_address": "fdfe:dcba:9876::1/126",
      "auto_route": true,
      "sniff": true,
      "sniff_override_destination": true
    }
  ],
  "outbounds": [
    {
      "type": "direct",
      "tag": "direct"
    },
    {
      "type": "block",
      "tag": "block"
    },
    {
      "type": "dns",
      "tag": "dns-out"
    },
    {
      "type": "selector",
      "tag": "select",
      "default": "auto",
      "outbounds": [
        "auto",
        "节点1",
        "节点2"
      ]
    },
    {
      "type": "urltest",
      "tag": "auto",
      "outbounds": [
        "节点1",
        "节点2"
      ],
      "url": "https://www.gstatic.com/generate_204",
      "interval": "5m",
      "tolerance": 100
    }
  ],
  "route": {
    "rules": [
      {
        "protocol": "dns",
        "outbound": "dns-out"
      },
      {
        "inbound": "tun-in",
        "port": 53,
        "outbound": "dns-out"
      },
      {
        "geoip": [
          "private",
          "cn"
        ],
        "outbound": "direct"
      },
      {
        "geosite": "cn",
        "outbound": "direct"
      },
      {
        "domain_suffix": [
          "google.com",
          "github.com",
          "youtube.com"
        ],
        "outbound": "select"
      },
      {
        "domain_keyword": [
          "proxy",
          "vpn"
        ],
        "outbound": "select"
      }
    ],
    "auto_detect_interface": true,
    "final": "select"
  }
}
